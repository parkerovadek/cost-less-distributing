{
  "name": "cld",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:importmap": "payload generate:importmap",
    "codegen": "graphql-codegen --config .graphql.ts",
    "codegen:watch": "graphql-codegen --config .graphql.ts --watch"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.8.0",
    "@payloadcms/email-nodemailer": "3.8.0",
    "@payloadcms/live-preview-react": "3.8.0",
    "@payloadcms/next": "3.8.0",
    "@payloadcms/plugin-form-builder": "3.8.0",
    "@payloadcms/plugin-nested-docs": "3.8.0",
    "@payloadcms/plugin-redirects": "3.8.0",
    "@payloadcms/plugin-seo": "3.8.0",
    "@payloadcms/richtext-lexical": "3.8.0",
    "@payloadcms/ui": "3.8.0",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@shopify/api-codegen-preset": "^1.1.2",
    "@shopify/storefront-api-client": "^1.0.3",
    "algoliasearch": "^5.15.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.3.1",
    "graphql": "^16.9.0",
    "jsonwebtoken": "9.0.2",
    "lexical": "0.18.0",
    "lucide-react": "^0.456.0",
    "next": "15.0.4",
    "payload": "3.8.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.53.2",
    "react-use": "^17.5.1",
    "sharp": "0.33.5",
    "shopify-buy": "^2.22.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@next/eslint-plugin-next": "^15.0.3",
    "@payloadcms/eslint-config": "^1.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.9.0",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@types/shopify-buy": "^2.17.4",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
